package com.itstart.demo02;
/**
 * 多重继承 演示super指向
 * @author xinyu
 *
 */
public class NEtiger extends Tiger {
	private static String name = "NEtiger";
	
	public NEtiger() {
		//super.getName();
		//super();
		System.out.println(name);
		
		//System.out.println(super.getName());
	}

	public static String getName() {
		return name;
	}

	public static void setName(String name) {
		NEtiger.name = name;
	}
}
