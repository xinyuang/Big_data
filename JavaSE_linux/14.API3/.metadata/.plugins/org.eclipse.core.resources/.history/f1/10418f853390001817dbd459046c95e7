package com.itstar.demo1;
/**
 * 声明一个抽象类，其中print为抽象方法
 * 
 * */
public abstract class Printer {
	private String name;
	private int size;
	public abstract void print();
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getSize() {
		return size;
	}
	public void setSize(int size) {
		this.size = size;
	}
	
}
