# start
`start-hbase.sh`

# stop
`stop-hbase.sh`

hbase shell
create 'sudent','info','grade'
put 'student','stu1','info:name','Tom'
put 'student','stu1','grade','99'
put 'student','stu2','info:name','Marry'
put 'student','stu2','grade','90'
get 'student','stu1'
    ...
put 'student','stu3','info:name','Bob'
    ...
