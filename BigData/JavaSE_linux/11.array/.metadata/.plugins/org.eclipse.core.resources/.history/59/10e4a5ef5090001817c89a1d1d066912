package TypeConverter;

public class array_ {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double[] d = new double[3];
		double data[] = new double[] {1.2, 2.3, 1.3};
		d[0] = 1.2;
		d[1] = 12.3;
		d[3] = 12.4;
		for (int i = 0; i < d.length; i++) {
			System.out.println(d[i]);
		}
		
	}

}
